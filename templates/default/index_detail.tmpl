{{define "page"}}
	<form method="POST" enctype="multipart/form-data">
		<input type="file" name="file" required />
		<input type="submit" name="submit" class="btn" value="Send" />
	</form>
	{{if .dataset}}
	<input type="button" id="process" value="Process" />
	<table class="table table-striped table-bordered table-hover">
		<tr>
		{{range $idx, $val := .dataset.Header}}
			<th>
				<label class="checkbox">
					{{$val.Name}}
					<input type="checkbox" data-id="{{$idx}}" class="checkField" />
				</label>
			</th>
		{{end}}
		</tr>
		{{$firstset := index .dataset.Data 0}}
		{{$data := .dataset.Data}}
		{{range $idx, $val := $firstset.Real}}
		<tr>
			{{range $set := $data}}
			<td>
				{{index $set.Real $idx}}
			</td>
			{{end}}
		</tr>
		{{end}}
	</table>
	<div>
		<div id="boxplot" style="height: 400px; margin: auto; min-width: 310px; max-width: 600px"></div>
		<div id="histogram" style="margin: auto; min-width: 310px; max-width: 600px"></div>
	</div>
	{{end}}
	<script type="text/javascript" src="/statics/js/index.js"></script>
	<script type="text/javascript" src="/statics/js/libs/highcharts.js"></script>
	<script type="text/javascript" src="/statics/js/libs/highcharts-more.js"></script>
{{end}}